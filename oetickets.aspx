<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html style="background-color:#000;">
	<head>
  		<title>OE Tickets</title>
  		<link rel="stylesheet" type="text/css" href="main.css">
  		<link rel="stylesheet" type="text/css" href="txtAnim.css">
	</head>
	<body>
		<div class="site">
			<div class="mainSite">
				<div class="title">
					<div class="cssload-wrap">
						<div class="cssload-bg">
							<div class="cssload-loading">
								<span class="cssload-title">OE TICKETS</span>
							</div>
						</div>
					</div>
				</div>
				<div id="emailSubject">
				</div>
				<div id="emailBody">
				</div>
				<div class="dataForm">
					<form>
						<table>
							<tr>
								<td>Assign to:</td>
								<td>
									<select name="assign" id="assign">
										<option value="-">-</option>
										<option value="Jamie">Jamie</option>
										<option value="Gareth">Gareth</option>
									</select>
								</td>
							</tr>
							<tr>
								<td>Due date:</td>
								<td>
									<input type="date" id="due">
								</td>
							</tr>
							<tr>
								<td>Start date:</td>
								<td>
									<input type="date" id="start">
								</td>
							</tr>
							<tr>
								<td>Finish date:</td>
								<td>
									<input type="date" id="finish">
								</td>
							</tr>
							<tr>
								<td>Time spent:</td>
								<td>
									<select name="time" id="name">
									<option value="-">-</option>
									<option value="0.25 days">0.25 days</option>
									<option value="0.5 days">0.5 days</option>
									<option value="0.75 days">0.75 days</option>
									<option value="1 day">1 day</option>
									<option value="1.25 days">1.25 days</option>
									<option value="1.5 days">1.5 days</option>
									<option value="1.75 days">1.75 days</option>
									<option value="2 days">2 days</option>
								</select>
								</td>
							</tr>
						</table>

							</li>
						</ul>
					</form>
				</div>
				<div class="comments">
					<h2>Comments</h2>
					<p>This is where the previous comments boxes will be.</p>
				</div>
				<div id="commentForm">
					<h2>Add a comment here</h2>
					<p>Add a comment here......</p>
				</div>
			</div>
		</div>
	</body>
</html>
